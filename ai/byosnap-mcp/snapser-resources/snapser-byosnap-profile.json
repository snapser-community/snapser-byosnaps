{
    "name": "Byosnap MCP",
    "description": "MCP Service",
    "platform": "linux/arm64",
    "language": "python",
    "prefix": "/v1",
    "ingress": {
        "external_port": {
            "name": "http",
            "port": 5003
        },
        "internal_ports": []
    },
    "readiness_probe_config": {
        "initial_delay_seconds": 5,
        "path": "/healthz"
    },
    "dev_template": {
        "cpu": 100,
        "memory": 0.125,
        "min_replicas": 1,
        "cmd": "",
        "args": [],
        "env_params": [
            {
                "key": "SNAPSER_ENVIRONMENT",
                "value": "DEVELOPMENT"
            }
        ]
    },
    "stage_template": {
        "cpu": 100,
        "memory": 0.125,
        "min_replicas": 1,
        "cmd": "",
        "args": [],
        "env_params": [
            {
                "key": "SNAPSER_ENVIRONMENT",
                "value": "STAGING"
            }
        ]
    },
    "prod_template": {
        "cpu": 100,
        "memory": 0.125,
        "min_replicas": 2,
        "cmd": "",
        "args": [],
        "env_params": [
            {
                "key": "SNAPSER_ENVIRONMENT",
                "value": "PRODUCTION"
            }
        ]
    }
}